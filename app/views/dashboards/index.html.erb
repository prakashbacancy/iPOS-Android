<section class="content-wrapper">
  <div class="content">
    <div class="add-item-btn mb-4">
    </div>

    <div class="box-body admin-box border-box mb-4">
      <div class="row align-items-center category-search admin-form inventory-item-search">
        <div class="search-wrap pl-3">
          <form action="#">
            <div class="row">
              <div class="col-lg-5 col-input">
                <div class="form-group search-input mb-0">
                  <%= text_field_tag :search, class: "form-control", placeholder: 'Search Customers' %>
                </div>
              </div>
              <div class="col-lg-1 col-btn">
                <%= submit_tag "Search", class: 'btn btn-admin', id: 'searchButton' %>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>

    <div class="row mt-4">
      <div class="col-12">
        <div class="row align-items-center category-search admin-form inventory-item-search justify-content-end mb-3"
          id="customer_pagination">
        </div>
        <div class="admin-table bg-white table-responsive">
          <table class="table table-bordered mb-0">
            <thead class="thead-dark">
              <tr>
                <th class="text-center">Customer List</th>
                <th class="text-center">Primary Details</th>
                <th class="text-center">Action</th>
              </tr>
            </thead>
            <tbody class="customer_list">
            </tbody>
          </table>
          <br>
        </div>
      </div>
    </div>

  </div>
</section>

